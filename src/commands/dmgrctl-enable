#!?

use base
use hook
use func
use handlers

function each() {
  if ! _exist $DMGR_SUPPORT $value; then
    echo "${e} is not supported. To support this feature, use 'dmgr support <feature_name>'"
    continue
  fi

  if _exist $DMGR_INUSE $value; then
    echo "${e} has already been enabled."
    continue
  fi

  # Load and call a hook callback.
  ! _hook enable $value && _reperr "Not successful. Aborted."

  _save $DMGR_INUSE $(_parse $DMGR_INUSE) $value
  echo "${e} has been enabled."
}

[ $c = false ] && exit 0
exit 200
